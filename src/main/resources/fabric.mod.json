{
  "schemaVersion": 1,
  "id": "tpshud",
  "version": "${version}",
  "name": "TPS Hud",
  "description": "A mod for fabric that displays the servers tps.",
  "authors": [
    "mooziii"
  ],
  "contact": {
    "sources": "https://github.com/mooziii/tpshud-fabric"
  },
  "license": "MIT",
  "icon": "assets/tpshud/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "me.obsilabor.tpshud.TpsHud"
    ],
    "modmenu": [
      "me.obsilabor.tpshud.ModMenuApiImpl"
    ],
    "server": [
      "me.obsilabor.tpshud.TpsHudServer"
    ]
  },
  "mixins": [
    "tpshud.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": ">=1.19",
    "cloth-config": "*"
  },
  "recommends": {
    "modmenu": ">=4.0.0",
    "silk-core": ">=1.9.0"
  }
}
